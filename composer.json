{
    "name": "bault/baultphp",
    "description": "A modern, modular PHP framework.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Bault.Dev",
            "email": "contact@bault.dev"
        }
    ],
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.5",
        "symfony/http-foundation": "^6.3",
        "symfony/var-dumper": "^6.3",
        "symfony/console": "^6.3",
        "nyholm/psr7": "^1.8",
        "spiral/roadrunner-http": "^3.3",
        "firebase/php-jwt": "^6.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "filp/whoops": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Core\\": "src/Core/",
            "Http\\": "src/Http/",
            "Modules\\": "Modules/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/Core/helpers.php"
        ]
    }
}