{
  "name": "my-ddd-framework/app",
  "type": "project",
  "require": {
    "php": "^8.1",
    "illuminate/events": "^10.0",
    "illuminate/container": "^10.0",

    "symfony/finder": "^6.0",
    "symfony/yaml": "^6.0",
    "doctrine/inflector": "^2.0",
    "ramsey/uuid": "^4.2",

    "league/flysystem": "^3.0",
    "league/event": "^3.0",
    "spatie/laravel-permission": "^6.0",   
    "spatie/laravel-event-sourcing": "^7.0",
    "webonyx/graphql-php": "^15.0",
    "phpoption/phpoption": "^1.9",
    "vlucas/phpdotenv": "^5.6",
    "symfony/console": "^6.4",
    "firebase/php-jwt": "^6.11",
    "filp/whoops": "^2.18",
    "nyholm/psr7": "^1.8",
    "illuminate/validation": "^10.48",
    "illuminate/translation": "^10.48"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "src/Core/",
      "Modules\\": "modules/"
    },
    "files": [
      "src/Core/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
        "Core\\": "src/Core/",
        "Modules\\": "modules/",
        "Http\\": "src/Http/",
        "App\\Providers\\": "src/Providers/",
        "App\\Exceptions\\": "src/Exceptions/"
    },
    "files": [
      "src/Core/helpers.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "spatie/phpunit-snapshot-assertions": "^5.0",
    "symfony/var-dumper": "^7.3"
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
